{
  "manifest_version": 2,
  "name": "Tabot",
  "short_name": "Tabot",
  "version": "1.8.1",
  "description": "Turns your webpage into a northerner.",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.1.min.js", "content.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_title": "Tabot",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
